    "yup-phone-lite": "^1.1.0"
  },
  "scripts": {
    "start": "next start",
    "build": "yarn tailwind:build && yarn next build",
    "dev": "concurrently \"yarn:tailwind:dev\" \"yarn:next dev\"",
    "build:tailwind": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "tailwind:dev": "cross-env TAILWIND_MODE=watch postcss src/styles/index.css -o src/styles/tailwind.css --watch",
    "tailwind:build": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "lint": "eslint --ignore-path .eslintignore \"**/*.{jsx,js,tsx}\" --fix",
    "lint:prettier": "pretty-quick",
    "style:all": "tsc && npm run style:lint && npm run style:prettier",
    "style:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "storybook": "yarn tailwind:build && start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "cypress:open": "cypress open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.2",
    "@types/axios": "^0.14.0",
    "@types/cypress": "^1.1.3",
    "@types/gulp": "^4.0.8",
    "@types/js-cookie": "^2.2.7",
    "@types/next": "^9.0.0",
    "@types/node": "^15.12.4",
    "@types/prettier": "2.2.1",
    "@types/prop-types": "^15.7.3",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-query": "^1.2.9",
    "@types/tailwindcss": "^2.0.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.7.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.2.1",
    "storybook": "^6.3.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint staged",
      "pre-commit": "lint staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
